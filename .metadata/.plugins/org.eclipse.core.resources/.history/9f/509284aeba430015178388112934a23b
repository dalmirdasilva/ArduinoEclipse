#include <Arduino.h>
#include <BMP085Barometer.h>


void setup() {
	Serial.begin(9600);
    Serial.println("initialized...");
	BMP085Barometer barometer = BMP085Barometer();
    Serial.println("barometer.getTemperature()");
    while(true) {
    Serial.println(barometer.getTemperature());
    delay(1000);
    }
}

void loop() {
}

int main(void) {

    init();

#if defined(USBCON)
	USBDevice.attach();
#endif

	setup();

	for (;;) {
		loop();
		if (serialEventRun)
			serialEventRun();
	}

	return 0;
}

