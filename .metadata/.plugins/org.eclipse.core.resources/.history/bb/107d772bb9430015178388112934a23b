#include <Arduino.h>
#include <BMP085Barometer.h>

BMP085Barometer barometer = BMP085Barometer();

void setup() {
	Serial.begin(9600);
}

void loop() {
	Serial.println("barometer.getTemperature()");
	//Serial.println(barometer.getTemperature());
	delay(1000);
}

int main(void) {

    init();

#if defined(USBCON)
	USBDevice.attach();
#endif

	setup();

	for (;;) {
		loop();
		if (serialEventRun)
			serialEventRun();
	}

	return 0;
}

