#include <Arduino.h>
#include <BMP085Barometer.h>

void setup() {
    Serial.begin(9600);
    Serial.println("initialized...");
    BMP085Barometer barometer = BMP085Barometer();
    while (true) {
        Serial.print("barometer.getTemperature(): ");
        Serial.println(barometer.getTemperature());

        Serial.print("barometer.getPressure(): ");
        Serial.println(barometer.getPressure());

        Serial.print("barometer.getAltitude(): ");
        Serial.println(barometer.getAltitude());

        delay(1000);
    }
}

void loop() {
}

int main(void) {

    init();

#if defined(USBCON)
    USBDevice.attach();
#endif

    setup();

    for (;;) {
        loop();
        if (serialEventRun)
            serialEventRun();
    }

    return 0;
}

