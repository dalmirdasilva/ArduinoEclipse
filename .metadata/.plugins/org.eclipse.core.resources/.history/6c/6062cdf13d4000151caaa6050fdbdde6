
void setup() {
    pinMode(13, OUTPUT);
}

void loop() {
    char state = digitalRead(13);
    digitalWrite(13, !state);
    delay(100);
}

int main(void) {
    init();

#if defined(USBCON)
    USBDevice.attach();
#endif

    setup();

    for (;;) {
        loop();
        if (serialEventRun) {
            serialEventRun();
        }
    }

    return 0;
}

